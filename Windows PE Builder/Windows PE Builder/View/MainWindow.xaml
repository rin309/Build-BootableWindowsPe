        <Grid Background="#337F7F7F" x:Name="CreateProjectRoot" Visibility="Hidden">
            <Border HorizontalAlignment="Center" VerticalAlignment="Center" Background="{DynamicResource {x:Static SystemColors.WindowBrushKey}}" BorderBrush="{DynamicResource BorderBrush}" BorderThickness="1">
                <StackPanel Margin="20">
                    <TextBlock FontSize="22">新しいプロジェクト</TextBlock>
                    <TextBlock Margin="0,20,0,0">プロジェクトの保存先</TextBlock>
                    <Grid Width="400" HorizontalAlignment="Left">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <TextBox x:Name="ProjectPathTextBox" IsReadOnly="True" Grid.Column="0"/>
                        <Button x:Name="ProjectPathButton" Content="参照" Grid.Column="1"/>
                    </Grid>
                    <TextBlock Margin="0,10,0,0">空のフォルダーを指定することをお勧めします</TextBlock>
                    <StackPanel Margin="0,20,0,0" Orientation="Horizontal" HorizontalAlignment="Right">
                        <Button x:Name="CreateProjectOkButton">OK</Button>
                    </StackPanel>
                </StackPanel>
            </Border>
        </Grid>

        <Grid x:Name="EulaRoot" Visibility="Hidden">
            <Grid Background="{DynamicResource BackgroundBrush}" VerticalAlignment="Center">
                <StackPanel Orientation="Vertical" MaxWidth="600">

                    <TextBlock Padding="{DynamicResource NormalThickness}" Foreground="White" Margin="0,10,0,0" FontSize="22"><Run Language="ja-jp" Text="作成したファイルは必ず動作確認を行ってください"/></TextBlock>
                    <StackPanel Orientation="Vertical" Margin="{DynamicResource GroupThickness}">
                        <TextBlock TextWrapping="Wrap"  Foreground="White" Text="このプログラムでは、最悪のケースとしてファイルの損失につながるファイルを作成します。"/>
                        <TextBlock TextWrapping="Wrap"  Foreground="White" Text="作成後のファイルを実行する前に必ずテスト環境で正常に動作するかの確認をし、運用を開始後も正常に使用できるか定期的に確認を行ってください。"/>
                        <TextBlock></TextBlock>
                        <TextBlock TextWrapping="Wrap"  Foreground="White" Text="このアプリを使用したトラブルについて、作成者はいかなる責任も負いません。" Margin="0,0,0,10"/>
                        <!--<CheckBox x:Name="DontShowMeCheckBox" Foreground="White" Margin="{DynamicResource GroupThickness}">今後、このメッセージを表示しない</CheckBox>-->
                        <StackPanel Orientation="Horizontal" Margin="{DynamicResource NormalThickness}" HorizontalAlignment="Right">
                            <Button x:Name="AgreeButton" Padding="{DynamicResource NormalThickness}" HorizontalAlignment="Center" Background="{DynamicResource AccentBrush}" BorderBrush="{DynamicResource BorderBrush}" Foreground="White" Margin="0,0,10,0" BorderThickness="2">上記の内容に同意します</Button>
                            <Button x:Name="ExitButton" Padding="{DynamicResource NormalThickness}" BorderThickness="2">内容に同意できませんので、アプリを終了します</Button>
                        </StackPanel>
                    </StackPanel>

                </StackPanel>
            </Grid>
        </Grid>
    </Grid>
